<p id="notice"><%= notice %></p>

<h1>My Photo Blog</h1>

    <% @posts.each do |post| %>

        <a href="<%= post_url(post)  %>"> <%= image_tag(post.image.standard.url) %></a>
        <h2> <%= link_to post.title, post %></h2>
        <%= post.description %>
        <br>
        <hr>
    <% end %>


<br>
